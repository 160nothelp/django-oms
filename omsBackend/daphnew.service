[Unit]
Description=Daphne Interface new Server

[Service]
ExecStartPre=/bin/bash -c 'cd /data/projects/oms/omsBackend'
ExecStart=/bin/bash -c 'cd /data/projects/oms/omsBackend && /root/.pyenv/shims/python manage.py runworker --threads 5 --only-channels=websocket.*'
Restart=always
KillSignal=SIGQUIT
Type=notify
NotifyAccess=all

[Install]
WantedBy=multi-user.target